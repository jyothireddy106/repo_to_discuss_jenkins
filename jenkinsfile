pipeline{
          agent any
            stages{
                    stage('one'){
                                    steps{
                                            echo'Running build phase...!'
                                    }
                    }
                     stage ('two'){
                                     steps{
                                                  input(Do you want to proceed..?)
                                     }
                     }
                      stage ('three'){
                                      when{
                                            not{
                                                  branch "master"
                                            }
                                      }     
                                      steps{
                                              echo "Hello world!Dis is ur jo!"
                                      }
                     }
            }
}
